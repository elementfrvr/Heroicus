<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="heroicus.loginController">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="581.0" prefWidth="982.0" VBox.vgrow="ALWAYS">
         <children>
            <Label layoutX="232.0" layoutY="14.0" text="Welcome to Heroicus!" />
            <Label layoutX="195.0" layoutY="79.0" text="Please Enter Your Login Details:" />
            <TextField id="txtUsername" fx:id="txtUsername" layoutX="221.0" layoutY="153.0" />
            <PasswordField id="txtPassword" fx:id="txtPassword" layoutX="221.0" layoutY="223.0" />
            <Label layoutX="93.0" layoutY="159.0" text="Username:" />
            <Label layoutX="96.0" layoutY="229.0" text="Password:" />
            <Button id="btnLogin" fx:id="btnLogin" defaultButton="true" layoutX="299.0" layoutY="324.0" mnemonicParsing="false" onAction="#logIn" text="Login" />
            <Label id="lblFeedback" fx:id="lblFeedback" layoutX="316.0" layoutY="276.0" textAlignment="CENTER" />
         </children>
    </AnchorPane>
  </children>
</VBox>
